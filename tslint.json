{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "prettier": true,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "max-classes-per-file": false,
    "interface-name": false,
    "no-bitwise": false
  }
}
